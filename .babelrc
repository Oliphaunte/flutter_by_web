{
  "env": {
    "development": {
      "presets": [
        ["env", { 
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }],
        "react",
        "flow"
      ],
      "plugins": [ 
        [ "transform-runtime", { "polyfill": false }],
        [ "transform-object-rest-spread" ],
        [ "transform-class-properties" ],
        [ "babel-plugin-root-import", { "rootPathPrefix": "@" } ],
      ]
    },
    "production": {
      "presets": [
        ["env", { 
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }],
        "react",
        "flow"
      ],
      "plugins": [
        [ "transform-runtime", { "polyfill": false }],
        [ "transform-object-rest-spread" ],
        [ "transform-class-properties" ],
        [ "babel-plugin-root-import", { "rootPathPrefix": "@" } ]
      ]
    }
  }
}